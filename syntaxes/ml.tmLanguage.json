{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ML",
	"patterns": 
	[
		{
			"include": "#comment"
		},
		{
			"include": "#keywords" 
		},
		{
			"include": "#strings" 
		},
		{
			"include": "#entity" 
		},
		{
			"include": "#invalid"
		},
		{
			"include": "#constant"
		},
		{
			"include": "#markup"
		},
		{
			"include": "#meta"
		},
		{
			"include": "#storage"
		},
		{
			"include": "#support"
		},
		{
			"include": "#variable"
		}
	],
	"repository": 
	{
		"comment": 
		{
			"name": "comment.line.ml",
			"begin": "\\;",
			"end": "\\.*$"
		},
		"keywords": 
