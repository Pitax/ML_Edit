			}]
		},
		"strings": 
		{
			"name": "string.quoted.double.ml",
			"begin": "\"",
			"end": "\"",
			"patterns": 
			[
				{
					"name": "constant.character.escape.ml",
					"match": "\\\\."
				}
			]
		},
		"entity": 
		{
			"patterns":
			[{
				"name": "entity.name.type.ml",
				"match": "\\b(verde)\\b"
			},
			{
				"name": "entity.name.function.ml",
				"match": "\\b(giallochiaro)\\b"
			}]
		},
		"invalid": 
		{
			"patterns": 
			[{
				"name": "invalid.ml",
				"match": "\\b(rosso)\\b"
			}]
		},
		"constant": 
		{
			"patterns": 
			[{
				"name": "constant.language.ml",
				"match": "\\b(blue)\\b"
			},
			{
				"name": "constant.character.escape.ml",
				"match": "\\b(RETURN|END DECLARE|TIMED:|FUNCTION:|PERMANENT STRING BYTE:|PERMANENT WORD:|PERMANENT BYTE:|STRING LONG:|STRING WORD:|STRING BYTE:|LONG:|WORD:|BYTE:|BIT:|CONSTANT:|TRANSLATE:|DECLARE| = |End Function|Function|if|elseif|end id|Module 24|Module 8|Module 2|End Module)\\b"
			}]
		},
		"markup": 
		{
			"patterns": 
			[{
				"name": "markup.ml",
				"match": "\\b(markupheading)\\b"

			}]
		},
		"storage": 
		{
